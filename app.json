{
  "name": "A17-MD",
  "description": "A WhatsApp bot based on Baileysjs library (Ts+Js) with Multi Device support",
  "repository": "https://github.com/Kai0071/A17",
  "logo": "https://wallpapercave.com/wp/wp6471201.jpg",
  "keywords": ["nodejs", "whatsapp bot", "automation", "multi-device", "baileysjs"],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "eco"
    }
  },
  "scripts": {
    "start": "node index.js"
  },
  "env": {
    "NODE_ENV": "production"
  },
  "engines": {
    "node": "14.x"
  }
}
